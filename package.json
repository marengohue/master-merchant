    "chai-as-promised": "^7.1.1",
    "coffee-loader": "^0.9.0",
    "coffeescript": "^2.3.0",
    "cson": "^5.1.0",
    "css-loader": "^0.28.11",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jsx-loader": "^0.13.2",
